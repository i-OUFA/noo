    <?php 
    $i = 25;
    function SquareRoot($i) {
        $x =$i/2;
        $y = 0;
        while ($x - $y){
            $y = $x;
            $x = ($x + $i / $x) /2;

        }
        return $x;
    }
        $SquareRoot = SquareRoot($i);
        echo $SquareRoot;

    ?>
